<query id="getDocumentSrlsbyMemberSrl" action="select">
    <tables>
        <table name="documents" />
    </tables>
    <columns>
        <column name="document_srl" />
    </columns>
    <conditions>
        <condition operation="equal" column="member_srl" var="member_srl" filter="number" notnull="notnull" />
    </conditions>
</query>